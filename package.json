{
  "name": "pring",
  "version": "0.5.2",
  "description": "Cloud Firestore model framework for TypeScript - Google",
  "main": "./src/pring.js",
  "types": "./types/pring.d.ts",
  "scripts": {
    "build": "tsc --outDir src pring.ts -d && mv src/pring.d.ts ./types/.",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1amageek/pring.ts.git"
  },
  "keywords": [
    "firestore",
    "firebase",
    "pring",
    "model",
    "typescript"
  ],
  "author": "1amageek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1amageek/pring.ts/issues"
  },
  "files": [
    "src",
    "types"
  ],
  "homepage": "https://github.com/1amageek/pring.ts#readme",
  "dependencies": {
    "@google-cloud/firestore": "^0.11.1",
    "@types/uuid": "^3.4.3",
    "firebase-functions": "^0.8.1",
    "reflect-metadata": "^0.1.10"
  },
  "contributors": [
    {
      "name": "Norikazu Muramoto",
      "email": "tmy0x3@icloud.com"
    }
  ],
  "devDependencies": {
    "@types/es6-promise": "0.0.33",
    "@types/jest": "^21.1.9",
    "@types/node": "^8.5.2",
    "jest": "^22.0.3",
    "ts-jest": "^22.0.0",
    "typings": "^2.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
